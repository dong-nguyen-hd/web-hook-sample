{
  "SystemInformation": {
    "ApplicationName": "WebHookSample",
    "Node": "1",
    "Version": "1.0"
  },
  "SerilogConfig": {
    "EnableConsoleLog": true,
    
    "EnableFileLog": false,
    "PathFileLog": "./logs/.json",
    
    "EnableElasticsearchLog": true,
    "ElasticsearchUri": "http://localhost:9200",
    "ElasticsearchIndexFormat": "web-hook-sample-{0:yyyy.MM.dd}",
    "ElasticsearchUsername": "admin"
  },
  "CacheConfig": {
    "UseMemoryCache": true,
    "RedisUri": "localhost:6379",
    "RedisInstanceName": "web-hook-sample-" // Add prefix to key
  },
  "AllowedHosts": "*"
}
